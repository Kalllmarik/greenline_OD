<?xml version="1.0" encoding="UTF-8"?>
<configuration
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">
    <input>
        <net-file value="greenline.net.xml"/>
        <additional-files value="vType.trips.xml,taz.add.xml,trips.trips.xml"/>
        <route-files value="vehicles.rou.xml"/>
    </input>
    <processing>
        <ignore-route-errors value="true"/>
        <report>
            <verbose value="true"/>
            <duration-log.statistics value="true"/>
            <no-step-log value="true"/>
        </report>
    </processing>
    <routing>
        <device.rerouting.adaptation-steps value="60"/>
    </routing>
    <time>
        <begin value="0"/>
        <end value="3600"/>
        <step-length value="0.1"/>
    </time>
    <gui_only>
        <gui-settings-file value="gui.settings.xml"/>
    </gui_only>
</configuration>